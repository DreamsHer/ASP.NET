
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>业务往来单位过期证书查询</title>
    <link href="~/Content/bootstrap-3.3.7-dist/css/bootstrap.min.css" rel="stylesheet" />
    @*CSS样式(包含默认皮肤样式)*@
    <link href="~/Content/jquery.bsgrid-1.37/merged/bsgrid.all.min.css" rel="stylesheet" />
    @*CSS皮肤(需引用于bsgrid.all.min.css之后)*@
    <link href="~/Content/jquery.bsgrid-1.37/css/skins/grid_bootstrap.min.css" rel="stylesheet" />
    <style type="text/css">
        .container {
            width: 100%;
            padding: 0;
        }

        .panel-heading {
            font-size: 16px;
        }

        .panel-body {
            padding-top: 0;
        }

            .panel-body label {
                font-weight: normal;
                font-size: 16px;
            }

        #MethodOfSettlingAccounts {
            width: 550px;
            border: 1px solid #999;
            height: 50px;
        }
    </style>
</head>
<body>
    <div class="container">
        <div class="panel panel-primary">
            <div class="panel-heading">业务往来单位过期证书查询</div>
            <div class="panel-body">
                <div class="table">
                    <ul class="nav nav-tabs tab-color-blue background-blue" id="myTab4">
                        <li class="active">
                            <a id="tbnavB" href="#tpB" data-toggle="tab" aria-expanded="true">业务往来单位过期证书查询</a>
                        </li>

                    </ul>
                    <div class="tab-content  padding-0" id="InEmployeeForm">
                        <div class="tab-pane active" id="tpB">
                            <div id="chartL" style="width: 100%; height: 500px; float: left;">

                                <div class="modal-body" style="background:#eddddd;">
                                    <div class="row table-responsive" style="height:400px;overflow:auto;margin-top:-15px">
                                        <table class="table" id="tabShangBiaoXinXi4" style="margin-top:2px;width:100%">
                                            <tr>
                                                <th w_num="total_line">序号</th>
                                                <th w_index="ContactCertificateID" w_hidden="true"></th>
                                                <th w_index="SupplierName">供货商</th>
                                                <th w_index="SupplierCHName">财务往来单位</th>
                                                <th w_index="ReleaseTimeStr">登记时间</th>
                                                <th w_index="ReleaseTimeStrr">到期时间</th>
                                                <th w_index="ContactCertificateName">证书名称</th>
                                                <th w_index="ContactCertificateHM">号码</th>


                                            </tr>
                                        </table>
                                    </div>
                                    </div>

                                </div>

                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <script src="~/Content/bootstrap-3.3.7-dist/js/jquery-2.0.3.min.js"></script>
        <script src="~/Content/bootstrap-3.3.7-dist/js/bootstrap.min.js"></script>
        @*jquery.bsgrid*@
        <script src="~/Content/jquery.bsgrid-1.37/js/lang/grid.zh-CN.min.js"></script>
        <script src="~/Content/jquery.bsgrid-1.37/merged/bsgrid.all.min.js"></script>
        <script src="~/Content/layer/layer.js"></script>@*对话框*@
        <script src="~/Content/js/customfunction.js"></script>@*封装js*@
        <script src="~/Content/js/jquery.form.js"></script>
        <script src="~/Content/bootstrap-3.3.7-dist/js/bootstrap-table.js"></script>
        <script>
        
        var tabShangBiaoXinXi4;
        $(function(){
           
            tabShangBiaoXinXi4 = $.fn.bsgrid.init('tabShangBiaoXinXi4', {
                url: '/SupplierManagement/SupplierCkeck/Selectzhengshu',
                autoLoad: false,
                stripeRows: true,//隔行变色
                rowHoverColor: true,//划过行变色
                rowSelectedColor: true,//是否显示选中行背景色
                pageSize: 10,
                pageSizeSelect: false,//是否选择分页页数下拉框
                pagingLittleToolbar: true,//精简的图标按钮分页工具条
                pagingToolbarAlign: "left"//分页工具条的显示位置


            });
            tabShangBiaoXinXi4.refreshPage();
          
        })
      


        </script>
</body>
</html>
