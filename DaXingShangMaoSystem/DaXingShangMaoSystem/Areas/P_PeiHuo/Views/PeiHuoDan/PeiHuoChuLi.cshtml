                                                        UptueWantHourShuZu.splice(i, 1);
                                                    }
                                                }
                                            }
                                        }
                                        else {
                                            var jiandengyu = RuKuJianShu - PeiHuoJian;
                                            var ShengYuJian = data[0].MumberOfPackages - jiandengyu;
                                            var xishudengyu = RuKuXiShu1 - PeiHuoXiShu;
                                            var DeXiShu = data[0].Subdivision - xishudengyu;
                                            UptueWantHourShuZu.push(([WareHouseDetiailID, data[0].WareHouseID, data[0].GoodsIDs, ShengYuJian, DeXiShu, data[0].YuanMumberOfPack, data[0].UnotXiShu]));

