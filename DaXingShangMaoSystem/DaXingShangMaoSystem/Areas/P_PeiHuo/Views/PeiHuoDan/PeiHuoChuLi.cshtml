                                var DeXiShu = RuKuXiShu - RuKuXiShu1;
                                UptueWantHourShuZu.push(([WareHouseDetiailID, WareHouseID, OrderFormDetailID, ShengYuJian, DeXiShu]));

                                for (var i = 0; i < UptueWantHourShuZu.length; i++) {
